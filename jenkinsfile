pipeline {
  agent
  stages {
    stage("build images") {
      steps {
        sh 'docker-compose build'
        echo 'Build complete'
      }
    stage("start containers") {
      steps {
        sh 'docker-compose up -d'
        echo 'Containers started'
      }
    /* stage('run SCA') {

    } */
    }
    }
  }
}
